//output: 
//
//     *****
//      ****
//       ***
//        **
//         *



#include <iostream>
#include<fstream>
using namespace std;

int main()
{
    for (int i = 5; i >=1; i--) {

        for (int space = 5; space >= i; space--) {
            cout << " ";
        }

        for (int a = 1; a <= i; a++) {
            cout << "*";
        }
        cout << endl;
    }

    return 0;
}
