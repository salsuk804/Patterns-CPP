//output: 
//
//*****
//*   *
//*   *
//*   *
//*****



#include <iostream>
#include<fstream>
using namespace std;

int main()
{
    for (int i = 1; i <= 5; i++) {

        for (int a = 1; a <= 5; a++) {
            if (a == 1 || a == 5||i==1||i==5) {
                cout << "*";
            }
            else {
                cout << " ";
            }
        }
        cout << endl;
    }

    return 0;
}
